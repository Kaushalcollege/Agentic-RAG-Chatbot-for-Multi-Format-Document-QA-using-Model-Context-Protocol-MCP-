{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/agent/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
